<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Buoy (plugin for WordPress)</title>
    <parser>
        <default-package-name>WordPress</default-package-name>
        <target>docs</target>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <files>
        <directory>.</directory>
        <ignore>includes/vendor/*</ignore>
        <ignore>vendor/*</ignore>
        <ignore>_site/*</ignore>
    </files>
</phpdoc>
